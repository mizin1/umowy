<?xml version="1.0" encoding="ISO-8859-1"?>

<fop version="FOP @version@">

  <base>.</base>
  <source-resolution>72</source-resolution>
  <target-resolution>72</target-resolution>
  
  <renderers>
  	<renderer mime="application/pdf">
  		<filterList>
  			<value>flate</value>
  		</filterList>
  		<fonts>
		   <font embed-url="fop/metrics/ubuntur.ttf" kerning="yes">
		   	<!--embed-url="fop/metrics/ubuntur.ttf">-->
		      <font-triplet name="ubuntu" style="normal" weight="normal"/>
		   </font>
		   <font embed-url="fop/metrics/ubuntub.ttf" kerning="yes">
		   	<!--embed-url="fop/metrics/ubuntub.ttf">-->
		      <font-triplet name="ubuntu" style="normal" weight="bold"/>
		   </font>
		   <font embed-url="fop/metrics/ubuntui.ttf" kerning="yes">
		   	<!--embed-url="fop/metrics/ubuntui.ttf">-->
		      <font-triplet name="ubuntu" style="italic" weight="normal"/>
		   </font>
		   <font embed-url="fop/metrics/ubuntubi.ttf" kerning="yes">
		   	<!--embed-url="fop/metrics/ubuntubi.ttf">-->
		      <font-triplet name="ubuntu" style="italic" weight="bold"/>
		   </font>
		</fonts>
  	</renderer>
  </renderers>
</fop>