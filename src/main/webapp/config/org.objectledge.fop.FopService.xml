<?xml version="1.0" encoding="ISO-8859-1"?>

<fop version="FOP @version@">

  <base>.</base>
  <source-resolution>72</source-resolution>
  <target-resolution>72</target-resolution>
  
  <renderers>
  	<renderer mime="application/pdf">
  		<filterList>
  			<value>flate</value>
  		</filterList>
  		<fonts>
		   <font embed-url="fop/fonts/arial.ttf" kerning="yes">
	      <font-triplet name="ttfarial" style="normal" weight="normal"/>
	   </font>
		</fonts>
  	</renderer>
  </renderers>
</fop>