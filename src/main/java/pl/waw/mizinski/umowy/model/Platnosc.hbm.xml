<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="pl.waw.mizinski.umowy.model.Platnosc" table="platnosc"
		lazy="true">
		<id name="nazwa" type="java.lang.String">
			<column name="nazwa" sql-type="varchar" />
		</id>
		<property name="dni" type="java.lang.Integer">
			<column name="dni" sql-type="int4" />
		</property>
		<property name="miesiace" type="java.lang.Integer">
			<column name="miesiace" sql-type="int4" />
		</property>
	</class>
</hibernate-mapping>
