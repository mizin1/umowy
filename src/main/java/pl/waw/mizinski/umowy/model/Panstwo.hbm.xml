<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="pl.waw.mizinski.umowy.model.Panstwo" table="panstwo"
		lazy="true">
		<id name="kod" type="java.lang.String">
			<column name="kod" sql-type="varchar" />
		</id>
		<property name="nazwa" type="java.lang.String">
			<column name="nazwa" sql-type="varchar" />
		</property>
		<property name="obywatelstwo" type="java.lang.String">
			<column name="obywatelstwo" sql-type="varchar" />
		</property>
	</class>
</hibernate-mapping>
