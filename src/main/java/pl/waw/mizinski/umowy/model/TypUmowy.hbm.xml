<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="pl.waw.mizinski.umowy.model.TypUmowy" table="typ_umowy"
		lazy="true">
		<id name="nazwa" type="java.lang.String">
			<column name="nazwa" sql-type="varchar" />
		</id>
		<property name="tytul" type="java.lang.String">
			<column name="tytul" sql-type="varchar" />
		</property>
		<property name="kosztUzyskaniaPrzychodu" type="java.lang.Integer">
			<column name="koszt_uzystkania_przychodu" sql-type="int4" />
		</property>
	</class>
</hibernate-mapping>
