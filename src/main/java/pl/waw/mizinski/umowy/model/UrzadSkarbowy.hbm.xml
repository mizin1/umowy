<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="pl.waw.mizinski.umowy.model.UrzadSkarbowy" table="urzad_skarbowy"
		lazy="true">
		<id name="nazwa" type="java.lang.String" >
			<column name="nazwa" sql-type="varchar(50)"/>
		</id>
		<property name="miejscowosc" type="java.lang.String">
			<column name="miejscowosc" sql-type="varchar(50)" />
		</property>
		<property name="ulica" type="java.lang.String">
			<column name="ulica" sql-type="varchar(50)" />
		</property>
		<property name="nrDomu" type="java.lang.String">
			<column name="nr_domu" sql-type="varchar(6)" />
		</property>
		<property name="nrMieszkania" type="java.lang.String">
			<column name="nr_mieszkania" sql-type="varchar(6)" />
		</property>
		<property name="kodPocztowy" type="java.lang.String">
			<column name="kod_pocztowy" sql-type="varchar(10)" />
		</property>
		<property name="poczta" type="java.lang.String">
			<column name="poczta" sql-type="varchar(50)" />
		</property>
	</class>
</hibernate-mapping>
