<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="pl.waw.mizinski.umowy.model.Reprezentant" table="reprezentant"
		lazy="true">
		<id name="nazwa" type="java.lang.String">
			<column name="nazwa" sql-type="varchar" />
		</id>
		<property name="biernik" type="java.lang.String">
			<column name="biernik" sql-type="varchar" />
		</property>
	</class>
</hibernate-mapping>
